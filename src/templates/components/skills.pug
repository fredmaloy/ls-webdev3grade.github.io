script(type="text/x-template")#skillsId
    .skills__level
        ul.skills__level-diagrams
            li.skills__level-diagrams__row(v-for="skill in skills" :key="skill.skillsGroup")
                skills-row(
                    :skill="skill"
                )
                
                
script(type="text/x-template")#skills-row
    h4.headline-fourth-level {{skill.skillsGroup}}
        ul.skills__level-diagrams__list
            li.skills__level-diagrams__elem(v-for="(skillPercent, skillName) in skill.skills")
                skill(
                    :skillName="skillName"
                    :skillPercent="skillPercent"
                )


script(type="text/x-template")#skill
    .skills__level-diagrams__skill
        svg.skills__level-diagrams__svg(viewBox='0 0 100 100')
            circle.circle.circle__under(cx='50' cy='50' r='40')
            circle.circle.circle__above(cx='50' cy='50' r='40' ref="color-circle")
        .skills__level-diagrams__tool {{skillName}}