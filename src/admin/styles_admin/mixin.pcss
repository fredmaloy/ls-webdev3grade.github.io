@mixin desktopHd() {
  @media screen and (min-width: 1800px) {
    @content;
  }
}

@mixin desktop() {
  @media screen and (max-width:1200px) {
    @content;
  }
} 

@mixin desktop1100() {
  @media screen and (max-width:1100px) {
    @content;
  }
}

@mixin tablets() {
  @media screen and (max-width: 768px) {
    @content;
  }
}

@mixin phones() {
  @media screen and (max-width: 480px) {
    @content;
  }
}